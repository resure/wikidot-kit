{
  "name": "wikidot-kit",
  "version": "4.2.1",
  "description": "Tool for extracting useful data from wikidot",
  "license": "MIT",
  "repository": "resure/wikidot-kit",
  "author": "Gadzhi Gadzhiev <resure@yandex.ru> (https://resure.net/)",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build"
  ],
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "prepublishOnly": "rm -rf build && npm run build && npm test",
    "test": "jest",
    "lint": "eslint lib --ext='ts'",
    "console": "node build/repl"
  },
  "keywords": [
    "wikidot"
  ],
  "dependencies": {
    "axios": "^0.21.1",
    "cheerio": "^0.22.0",
    "p-queue": "^4.0.0",
    "promise-retry": "^1.1.1",
    "qs": "^6.9.4",
    "xmlrpc": "^1.3.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.33",
    "@types/cheerio": "^0.22.23",
    "@types/node": "^11.15.43",
    "@types/promise-retry": "^1.1.3",
    "@types/qs": "^6.9.5",
    "@types/xmlrpc": "^1.3.5",
    "eslint": "^7.0.0",
    "eslint-config-rs": "^1.0.0",
    "husky": "^4.3.7",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "2.2.1",
    "typescript": "^3.9.7"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  }
}
